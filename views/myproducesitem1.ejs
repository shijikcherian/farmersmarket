      <script src="../assets/js/html5shiv.js"></script>
    <![endif]-->

    <!-- Fav and touch icons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="http://getbootstrap.com/2.3.2/assets/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="http://getbootstrap.com/2.3.2/assets/ico/apple-touch-icon-114-precomposed.png">
      <link rel="apple-touch-icon-precomposed" sizes="72x72" href="http://getbootstrap.com/2.3.2/assets/ico/apple-touch-icon-72-precomposed.png">
                    <link rel="apple-touch-icon-precomposed" href="http://getbootstrap.com/2.3.2/assets/ico/apple-touch-icon-57-precomposed.png">
                                   <link rel="shortcut icon" href="http://getbootstrap.com/2.3.2/assets/ico/favicon.png">
    
  </head>
  <body>

    <p>Welcome to <%= title %></p>
    <div class="navbar navbar-inverse navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
	      <button type="button" class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
	            <span class="icon-bar"></span>
	            <span class="icon-bar"></span>
	            <span class="icon-bar"></span>
	       </button>
	       <a class="brand" href="#">Virtual Farmers Market</a>
	       <div class="nav-collapse collapse">
	            <ul class="nav">
	              <li class="active"><a href="/index">Home</a></li>
	              <li><a href="#about">Buy</a></li>
	              <li><a href="#about">Sell</a></li>
	              <li><a href="#about">About</a></li>
	              <li><a href="#contact">Contact</a></li>
	              <li class = "pull-right"> <a href="/logout">logout</a></li>
	            </ul>
	 
	        </div><!--/.nav-collapse -->

	    </div><!--/.container -->
	 </div><!--/.navbar-inner -->
   </div><!--/.navbar navbar-inverse navbar-fixed-top -->
	    
	
    <div class="container">	
      
      <div class="page-header">
	    <img src="/bootstrap/img/heading-background1.jpg"/>
      </div><!--/.pageheader -->
	
	
	  <!-- Example row of columns -->
	     
	     
      <div class="row">
        <div class="span2">      	  
          <ul class="nav nav-list">
            <li class="nav-header">What we are?</li>
            <li><a href="/">Home</a></li>
            <li><a href="#">Create Account</a></li>
            <li class="divider"></li>    
            <li class="active"><a href="/produces">Our Produces</a></li>
            <li><a href="/farmers">Our Farmers</a></li>
            <li class="divider"></li> 
            <li><a href="#">Share</a></li>       
          </ul>
        </div><!--/.span2 -->
        <div class="span6">
		  <form name="buyform<%=itemid%>" id="buyform<%=itemid%>" action="delete_item"method="post" >
			<h2>Welcome to <%= farmname %></h2>
			<p><%= description %></p>		

				
            <div class="table-responsive">
          
			  <table class="table table-condensed"> 
				      
		        <tbody> 
		          <tr>  
		              <td >
    		              <img src=<%= itemimage %> />
    		              <h4> <%= itemid %> <h4> 
		              </td>  
		    	              
		          </tr>  
                  <tr>  
                    <td ><%= description %> </td> 
                  </tr>  
	         
		          <tr>  
		            <th width ="10%">Per Pack:</th>  
		            <td ><%= perpack %> </td>  
		            <th >Unit Price</th>  
		            <td ><%= unitprice %></td>
		          </tr>  
		          <tr>  
		            <th >In Stock:</th>  
		            <td ><%= instock %></td>  
		            <th >Best By:</th>  
		            <td ><%= bestby %></td>  
		          </tr> 
                  <tr>  
                    <th >Item Type:</th>  
                    <td ><%= itemtype %></td>  
                    <th >Add Date:</td>  
                    <td ><%= adddate %></td>  
                  </tr> 
                  <tr>  
                    <th >Farm Name:</td>  
                    <td ><%= farmname %></td>  
                    <th >Tag List:</td>  
                    <td ><%= taglist %></td>  
                  </tr> 
		          <tr>

        			 <td>
					     
				          	<input type="hidden" name="itemid" value="<%=itemid%>"/>
				       </td>
				          <td>	
				            <button type="submit" class="btn btn-danger" name="action" value="delete">DELETE!</button>
				           </td>
				           <td> 
				            
                            <button type="submit" class="btn btn-success" name="action" value="update">UPDATE!</button>				            
				         					  
					 </td>		 
		          </tr>
		        </tbody>  
      		</table>  
      	
          </div>  <!-- /table responsive -->
       </div><!-- /span6 -->
        <div class="span4">
          <h2>Our recipe.....</h2>
          <iframe width="350" height="300" src=<%= videolink1 %> frameborder="0" allowfullscreen></iframe>
          <h2>Our farm....</h2>
          <iframe width="350" height="300" src=<%= videolink2 %> frameborder="0" allowfullscreen></iframe>
	    </div><!-- .span4 -->
      </form>  
        <hr>
 
      <footer>
        <p>Â© SJSU Fall 2013</p>
      </footer>
    
        </div><!-- /row -->
    </div> <!-- /container -->

